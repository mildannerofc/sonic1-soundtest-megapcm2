00071890                            ; ==============================================================================
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; Macros
00071890                            ; ------------------------------------------------------------------------------
00071890                            
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; Macro to generate sample record in a sample table
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; ARGUMENTS:
00071890                            ;	type - Sample type (TYPE_PCM, TYPE_DPCM, TYPE_PCM_TURBO or TYPE_NONE)
00071890                            ;	samplePtr - Sample pointer/name (assigned via `incdac` macro)
00071890                            ;	sampleRateHz? - (Optional) Playback rate in Hz, auto-detected for .WAV
00071890                            ;	flags? - (Optional) Additional flags (e.g. FLAGS_SFX or FLAGS_LOOP)
00071890                            ; ------------------------------------------------------------------------------
00071890                            
00071890                            dcSample: macro	type, samplePtr, sampleRateHz, flags
00071890                            	if narg>4
00071890                            		inform 2, "Too many arguments. USAGE: dcSample type, samplePtr, sampleRateHz, flags"
00071890                            	endif
00071890                            	dc.b	\type					; $00	- type
00071890                            	if \type=TYPE_PCM
00071890                            		if \sampleRateHz+0>TYPE_PCM_MAX_RATE
00071890                            			inform 2, "Invalid sample rate: \sampleRateHz\. TYPE_PCM only supports sample rates <= \#TYPE_PCM_MAX_RATE Hz"
00071890                            		endif
00071890                            		dc.b	\flags+0								; $01	- flags (optional)
00071890                            		dc.b	(\sampleRateHz+0)*256/TYPE_PCM_BASE_RATE; $02	- pitch (optional for .WAV files)
00071890                            		dc.b	0										; $03	- <RESERVED>
00071890                            		dc.l	\samplePtr								; $04	- start offset
00071890                            		dc.l	\samplePtr\_End							; $08	- end offset
00071890                            	elseif \type=TYPE_PCM_TURBO
00071890                            		if (\sampleRateHz+0<>TYPE_PCM_TURBO_MAX_RATE)&(\sampleRateHz+0<>0)
00071890                            			inform 2, "Invalid sample rate: \sampleRateHz\. TYPE_PCM_TURBO only supports sample rate of \#TYPE_PCM_TURBO_MAX_RATE Hz"
00071890                            		endif
00071890                            		dc.b	\flags+0								; $01	- flags (optional)
00071890                            		dc.b	$FF										; $02	- pitch (optional for .WAV files)
00071890                            		dc.b	0										; $03	- <RESERVED>
00071890                            		dc.l	\samplePtr								; $04	- start offset
00071890                            		dc.l	\samplePtr\_End							; $08	- end offset
00071890                            	elseif \type=TYPE_DPCM
00071890                            		if \sampleRateHz>TYPE_DPCM_MAX_RATE
00071890                            			inform 2, "Invalid sample rate: \sampleRateHz\. TYPE_DPCM only supports sample rates <= \#TYPE_DPCM_MAX_RATE Hz"
00071890                            		endif
00071890                            		dc.b	\flags+0								; $01	- flags (optional)
00071890                            		dc.b	(\sampleRateHz)*256/TYPE_DPCM_BASE_RATE	; $02	- pitch
00071890                            		dc.b	0										; $03	- <RESERVED>
00071890                            		dc.l	\samplePtr								; $04	- start offset
00071890                            		dc.l	\samplePtr\_End							; $08	- end offset
00071890                            	elseif \type=TYPE_NONE
00071890                            		dc.b	0, 0, 0
00071890                            		dc.l	0, 0
00071890                            	else
00071890                            		inform 2, "Unknown sample type. Please use one of: TYPE_PCM, TYPE_DPCM, TYPE_PCM_TURBO, TYPE_NONE"
00071890                            	endif
00071890                            	endm
00071890                            
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; Macro to include a sample file
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; ARGUMENTS:
00071890                            ;	name - Name assigned to the sample (label)
00071890                            ;	path - Sample's include path (string)
00071890                            ; ------------------------------------------------------------------------------
00071890                            
00071890                            incdac:	macro name, path
00071890                            		even
00071890                            	\name:
00071890                            		incbin	\path
00071890                            	\name\_End:
00071890                            	endm
00071890                            
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; Macro to stop Z80 and take over its bus
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; ARGUMENTS:
00071890                            ;	opBusReq? - (Optional) Custom operand for Z80_BUSREQ
00071890                            ; ------------------------------------------------------------------------------
00071890                            
00071890                            MPCM_stopZ80:	macro opBusReq
00071890                            	pusho
00071890                            	opt		l-		; make sure "@" marks local labels
00071890                            	if narg=1
00071890                            		move.w	#$100, \opBusReq
00071890                            		@wait\@:
00071890                            			btst	#0, \opBusReq
00071890                            			bne.s	@wait\@
00071890                            	else
00071890                            		move.w	#$100, MPCM_Z80_BUSREQ
00071890                            		@wait\@:
00071890                            			btst	#0, MPCM_Z80_BUSREQ
00071890                            			bne.s	@wait\@
00071890                            	endif
00071890                            	popo
00071890                            	endm
00071890                            
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; Macro to start Z80 and release its bus
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; ARGUMENTS:
00071890                            ;	opBusReq? - (Optional) Custom operand for Z80_BUSREQ
00071890                            ; ------------------------------------------------------------------------------
00071890                            
00071890                            MPCM_startZ80:	macro opBusReq
00071890                            	if narg=1
00071890                            		move.w	#0, \opBusReq
00071890                            	else
00071890                            		move.w	#0, MPCM_Z80_BUSREQ
00071890                            	endif
00071890                            	endm
00071890                            
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; Ensures Mega PCM 2 isn't busy writing to YM (other than DAC output obviously)
00071890                            ; ------------------------------------------------------------------------------
00071890                            ; ARGUMENTS:
00071890                            ;	opBusReq? - (Optional) Custom operand for Z80_BUSREQ
00071890                            ; ------------------------------------------------------------------------------
00071890                            
00071890                            MPCM_ensureYMWriteReady:	macro opBusReq
00071890                            	pusho
00071890                            	opt		l-		; make sure "@" marks local labels
00071890                            	@chk_ready\@:
00071890                            		tst.b	(MPCM_Z80_RAM+Z_MPCM_DriverReady).l
00071890                            		bne.s	@ready\@
00071890                            		MPCM_startZ80 \opBusReq
00071890                            		move.w	d0, -(sp)
00071890                            		moveq	#10, d0
00071890                            		dbf		d0, *						; waste 100+ cycles
00071890                            		move.w	(sp)+, d0
00071890                            		MPCM_stopZ80 \opBusReq
00071890                            		bra.s	@chk_ready\@
00071890                            	@ready\@:
00071890                            	popo
00071890                            	endm
